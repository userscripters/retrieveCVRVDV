{
  "name": "@userscripters/close-reopen-delete-vote-retriever",
  "version": "1.1.1",
  "description": "Gets the close vote count for people without the close vote priviledge",
  "scripts": {
    "test": "mocha",
    "lint": "eslint src --fix --ext .js,.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run lint && npm run clean",
    "uglify-modern": "uglifyjs dist/modern/* --compress --mangle -o dist/modern/index.min.js",
    "uglify-legacy": "uglifyjs dist/legacy/* --compress --mangle -o dist/legacy/index.min.js",
    "build-modern": "tsc --outDir dist/modern && npm run uglify-modern",
    "build-legacy": "tsc --target es5 --outDir dist/legacy && npm run uglify-legacy",
    "build-all": "bash ./scripts/build.sh",
    "headers-tm": "bash ./scripts/headers.sh",
    "readme": "generate-readme",
    "postversion": "bash ./scripts/postversion.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/userscripters/retrieveCVRVDV.git"
  },
  "keywords": [
    "userscripts",
    "stackoverflow"
  ],
  "author": {
    "name": "Daniil"
  },
  "contributors": [
    {
      "name": "Oleg Valter",
      "email": "oleg.a.valter@gmail.com"
    }
  ],
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/userscripters/retrieveCVRVDV/issues"
  },
  "homepage": "https://github.com/userscripters/retrieveCVRVDV#readme",
  "devDependencies": {
    "@types/chai": "^4.3.17",
    "@types/chai-as-promised": "^7.1.8",
    "@types/jquery": "^3.5.30",
    "@types/mocha": "^10.0.7",
    "@types/node": "^20.14.14",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@userscripters/generate-headers": "^3.4.1",
    "@userscripters/generate-readme": "^3.0.2",
    "@userscripters/stackexchange-global-types": "^2.8.0",
    "chai": "^5.1.1",
    "eslint": "^8.57.0",
    "mocha": "^10.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "uglify-js": "^3.19.1"
  }
}
